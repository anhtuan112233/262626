public class BaitapLAB3 {

    public static void main(String[] args) {
        System.out.println("Hello World!");
    }
}
Public class Square {
    // Thuộc tính 
    private int length;

    // Phương thức get
    public int getLength() {
        return this.length;
    }

    // Phương thức set
    public void setLength(int length) {
        this.length = length;
    }
}

public class Crirle {
    // Thuộc tính 
    private int radius;

    // Phương thức get
    public int getRadius() {
        return this.radius;
    }

    // Phương thức set
    public void setRadius(int radius) {
        this.radius = radius;
    }
}
