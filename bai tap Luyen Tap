public class HoaQua {
 
  public String mau;
 public float gia;
 public int soluong;
 public String ngayNhap;
 public String Nguongoc;
 
 public HoaQua(String mau, float gia, int soluong, String ngayNhap, String Nguongoc) {
            this.mau = mau;
            this.gia = gia;
            this.soluong = soluong;
            this.ngayNhap = ngayNhap;
            this.Nguongoc = Nguongoc;
        }
 
 public void mau (){
     System.out.println( "Nhap mau cua qua : " );
     System.out.println(" Mau cua qua la :"+ mau);
     
     }
     public void gia(){
         System.out.println("Nhap gia cua cam ");
         System.out.println("Gia cua qua cam la" + gia);
     }
     public void soluong(){
         System.out.println(" Nhap so luong qua cam :");
         System.out.println(" So luong qua cam la :"+ soluong);
     }
      public void ngayNhap(){
          System.out.println("Ngay qua cam nhap ");
          System.out.println("Ngay nhap qua cam :" +ngayNhap);
         
      }
      public void Nguongoc(){
          System.out.println("Nguon goc qua cam :");
           System.out.println("Nguon goc cua qua cam la :"+ Nguongoc);
      }
      
      
       class Quacam extends HoaQua{
         
      public String Huongvi;

        public Quacam(String mau, float gia, int soluong, String ngayNhap, String Nguongoc) {
            super(mau, gia, soluong, ngayNhap, Nguongoc);
        }

      public void Huongvi(){
          System.out.println("Nhap huong vi qua cam");
          this.Huongvi = Huongvi;
          System.out.println("Huong vi qua cam:"+ Huongvi);
      }
     
 }
    class quaTao extends HoaQua{
      public String voNgoai;
       

        public quaTao(String mau, float gia, int soluong, String ngayNhap, String Nguongoc) {
            super(mau, gia, soluong, ngayNhap, Nguongoc);
        }
        public void Huongvi(){
      System.out.println(" Vo ngoai qua cam nhu nao :");
      this.voNgoai = voNgoai;
      System.out.println(" qua tao co vo ngoai :"+ voNgoai);
       
        }
    
    }
    class CamCaophong extends HoaQua{
        public float luongdinhduong;
        
        public CamCaophong(String mau, float gia, int soluong, String ngayNhap, String Nguongoc) {
            super(mau, gia, soluong, ngayNhap, Nguongoc);
        }
       public void luongdinhduong(){
           System.out.println(" luong dinh duong cua qua cam cao phong:");
           this.luongdinhduong = luongdinhduong;
           System.out.println("qua cam cao phong co luong dinh duong la "+luongdinhduong);
       }
        
    }
         
}
